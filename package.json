{
  "name": "stroll-the-dice",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=local webpack serve --mode development --open --hot",
    "clean": "rm -rf public",
    "build": "cross-env NODE_ENV=development webpack --mode production",
    "build:prod": "cross-env NODE_ENV=production webpack --mode production",
    "deploy": "npm run clean && npm run build && firebase deploy --only hosting",
    "dp": "npm run clean && npm run build:prod && firebase deploy --only hosting",
    "da": "npm run set:dev && npm run deploy && npm run set:prod && npm run dp",
    "set:dev": "firebase use stroll-the-dice",
    "set:prod": "firebase use stroll-the-dice",
    "projects": "firebase projects:list",
    "pd": "git push && npm run deploy",
    "pda": "git push && npm run da"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheVVaFFle/stroll-the-dice.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TheVVaFFle/stroll-the-dice/issues"
  },
  "homepage": "https://github.com/TheVVaFFle/stroll-the-dice#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "copy-webpack-plugin": "^9.0.0",
    "firebase": "^8.6.2",
    "lodash.sortby": "^4.7.0",
    "nanoid": "^3.1.23",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.3.0",
    "@types/lodash.sortby": "^4.7.6",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.1.7",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.6.0",
    "sass": "^1.34.0",
    "sass-loader": "^11.1.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
